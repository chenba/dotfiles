unsetenv DISPLAY
setenv LC_CTYPE en_US.UTF-8

hardstatus alwayslastline
hardstatus string '%{= kG}[ %{G}%H %{g}][%= %{= kw}%?%-Lw%?%{r}(%{W}%n*%f%t%?(%u)%?%{r})%{w}%?%+Lw%?%?%= %{g}][%{B} %m/%d %{W}%c %{g}]'
termcapinfo xterm-color|rxvt ti@:te@
term xterm-color

defmonitor on
defscrollback 5000
defutf8 on
vbell off
